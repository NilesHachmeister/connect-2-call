{
  "name": "connect-2-call",
  "version": "1.0.0",
  "description": "",
  "main": "server/server.js",
  "homepage": "https://connect-2-caller.herokuapp.com/",
  "scripts": {
    "start": "node --optimize_for_size --max_old_space_size=460 server/server.js",
    "develop": "node --optimize_for_size --max_old_space_size=460 concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "seed": "cd server && npm run seed",
    "build": "cd client && npm run build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^5.1.0"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.6",
    "bcryptjs": "^2.4.3",
    "i18next": "^21.8.9",
    "i18next-browser-languagedetector": "^6.1.4",
    "i18next-http-backend": "^1.4.1",
    "i18next-locize-backend": "^5.1.2",
    "react-i18next": "^11.17.1"
  }
}